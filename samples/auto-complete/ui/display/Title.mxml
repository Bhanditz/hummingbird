<s:RichText
	xmlns:fx="http://ns.adobe.com/mxml/2009" 
	xmlns:s="library://ns.adobe.com/flex/spark" 
	xmlns:mx="library://ns.adobe.com/flex/halo"
	width="100%">
	<s:filters>
		<s:BevelFilter distance="0.5" highlightColor="0x000000" shadowColor="0xffffff"/>
	</s:filters>
	<s:textFlow>
		<s:TextFlow textAlign="center" color="0x999999" fontWeight="bold" >
			<s:p fontSize="60" paragraphSpaceAfter="20">Hummingbird</s:p>
			<s:p fontSize="30">AutoComplete Example</s:p>
		</s:TextFlow>
	</s:textFlow>
</s:RichText>